{
  "name": "annual-teamup",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "bash -lc 'set -a; [ -f .env.local ] && source .env.local; set +a; drizzle-kit generate'",
